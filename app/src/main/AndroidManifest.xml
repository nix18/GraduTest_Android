<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.myapp.gradutest_android">
    <!-- 网络权限 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" /> <!-- 日程权限 -->
    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" /> <!-- 存储权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_app_icon"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@drawable/ic_app_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity android:name=".Data_Analyse_Activity"></activity>
        <activity android:name=".Exchange_Goods_Activity" />
        <activity android:name=".Share_Img_Activity" />
        <activity android:name=".All_Habits_Activity" />
        <activity android:name=".Credit_Lottery_Activity" />
        <activity android:name=".My_Credit_Detail_Activity" />
        <activity android:name=".My_Habits_Activity" />
        <activity android:name=".Settings_Activity" />
        <activity android:name=".My_Credit_Activity" />
        <activity android:name=".My_Info_Activity" />
        <activity android:name=".Add_Habit_Activity" />
        <activity android:name=".Test_Toolbar_Activity" />
        <activity android:name=".Habit_Info_Activity" />
        <activity android:name=".Update_Info_Activity" />
        <activity android:name=".Screen_Error_Activity" />
        <activity android:name=".Log_In_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Sign_In_Activity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>